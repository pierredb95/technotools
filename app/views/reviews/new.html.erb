<div class="contain devise-form">
  <%= simple_form_for [ @booking, @review ] do |f| %>
    <%= f.input :comment %>
    <%= f.input :rating, as: :integer %>
    <%= f.submit class: 'btn btn-primary' %>
  <% end %>
</div>


